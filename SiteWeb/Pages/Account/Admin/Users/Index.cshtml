@page
@model IndexModel
@{
	ViewData["Title"] = "Users";
}

<main>
	<section class="container">
		<div style="padding: 20px 0px;">
			<h4><a asp-page="/Account/Admin/Index">Menu</a>   &gt;   Users</h4>
		</div>
	</section>

	<section class="high-priority">
		<div class="container">
			<div class="row">
				<div class="lg-12">
					<div class="high-priority-header">
						<h1 class="h4">Users</h1>
					</div>
					<div class="high-priority-table">
						<table class="table dt-responsive cstm-datatable nowrap" id="usersTable" cellspacing="0">
							<thead>
								<tr>
									<th>Name</th>
									<th>Email address</th>
									<th>Status</th>
									<th>Membership</th>
									<th>Date Joined</th>
								</tr>
							</thead>
							<tbody>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</section>
</main>